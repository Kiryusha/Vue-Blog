mobile()
  @media only screen and (max-width: 1100px)
    block

phone()
  @media only screen and (max-width: 486px)
    block

no-select()
  pointer-events none
  user-select none

a-reset()
  text-decoration: none
  color inherit

ul-reset()
  list-style none
  margin: 0
  padding: 0

button-reset()
  background: none
  border: 0
  padding: 0
  outline none
  cursor: pointer
